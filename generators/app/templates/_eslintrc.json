{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "bundle": false,
        "dizmo": false,
        "viewer": false,
        "DizmoElements": false
    },
    "parserOptions": {
        "ecmaFeatures": {
            "generators": true,
            "jsx": true
        },
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "settings": {
        "react": {
            "version": "999.999.999"
        }
    }
}
